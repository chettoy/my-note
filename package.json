{
  "name": "my-note",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://chettoy.github.io/my-note",
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "@mdi/react": "^1.5.0",
    "axios": "^0.21.1",
    "dompurify": "^2.2.6",
    "katex": "^0.13.0",
    "marked": "^2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.1",
    "rich-markdown-editor": "^11.13.0",
    "sass": "^1.35.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3",
    "velocity-animate": "^1.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "fmt:extract": "formatjs extract 'src/**/*.js*' --out-file src/lang/en.json",
    "fmt:compile": "formatjs compile 'src/lang/en.json' --ast --out-file src/lang/compiled/en.json",
    "fmt:compile:zh": "formatjs compile 'src/lang/zh.json' --ast --out-file src/lang/compiled/zh.json",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@formatjs/cli": "^4.2.20",
    "gh-pages": "^3.1.0"
  }
}
